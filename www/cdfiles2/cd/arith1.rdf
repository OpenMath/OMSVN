      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/logic1#implies"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/logic1#and"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/set1#in"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/setname1#Z"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/set1#in"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/setname1#Z"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/logic1#not"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/quant1#exists"/>
