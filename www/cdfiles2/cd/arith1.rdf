   <ns1:SymbolDefinition rdf:about="http://www.openmath.org/cd/arith1#unary_minus">
      <ns3:identifier>unary_minus</ns3:identifier>
      <ns1:role rdf:resource="http://www.openmath.org/ontology#Application"/>
