   <ns1:SymbolDefinition rdf:about="http://www.openmath.org/cd/arith1#lcm">
      <ns3:identifier>lcm</ns3:identifier>
      <ns1:role rdf:resource="http://www.openmath.org/ontology#Application"/>
      <ns3:description>The symbol to represent the n-ary function to return the least common multiple of its arguments.</ns3:description>
      <ns1:hasProperty rdf:resource="http://www.openmath.org/cd/arith1#lcm.prop0"/>
      <ns1:hasProperty rdf:resource="http://www.openmath.org/cd/arith1#lcm.prop2"/>
      <ns4:sameAs rdf:resource="http://dbpedia.org/resource/Least_common_multiple"/>
   </ns1:SymbolDefinition>
   <ns1:Property rdf:about="http://www.openmath.org/cd/arith1#lcm.prop0">
      <ns1:text>lcm(a,b) = a*b/gcd(a,b)</ns1:text>
      <ns1:openMath rdf:parseType="Literal">
         <OMOBJ xmlns="http://www.openmath.org/OpenMath"
                xmlns:rxr="http://ilrt.org/discovery/2004/03/rxr/"
                version="2.0"
                cdbase="http://www.openmath.org/cd">
            <OMA>
               <OMS cd="relation1" name="eq"/>
               <OMA>
                  <OMS cd="arith1" name="lcm"/>
                  <OMV name="a"/>
                  <OMV name="b"/>
               </OMA>
               <OMA>
                  <OMS cd="arith1" name="divide"/>
                  <OMA>
                     <OMS cd="arith1" name="times"/>
                     <OMV name="a"/>
                     <OMV name="b"/>
                  </OMA>
                  <OMA>
                     <OMS cd="arith1" name="gcd"/>
                     <OMV name="a"/>
                     <OMV name="b"/>
                  </OMA>
               </OMA>
            </OMA>
         </OMOBJ>
      </ns1:openMath>
      <ns1:contentMathML rdf:parseType="Literal">
         <math xmlns="http://www.w3.org/1998/Math/MathML"
               xmlns:rxr="http://ilrt.org/discovery/2004/03/rxr/">
            <apply>
               <csymbol cd="relation1">eq</csymbol>
               <apply>
                  <csymbol cd="arith1">lcm</csymbol>
                  <ci>a</ci>
                  <ci>b</ci>
               </apply>
               <apply>
                  <csymbol cd="arith1">divide</csymbol>
                  <apply>
                     <csymbol cd="arith1">times</csymbol>
                     <ci>a</ci>
                     <ci>b</ci>
                  </apply>
                  <apply>
                     <csymbol cd="arith1">gcd</csymbol>
                     <ci>a</ci>
                     <ci>b</ci>
                  </apply>
               </apply>
            </apply>
         </math>
      </ns1:contentMathML>
      <ns1:popcorn>arith1.lcm($a, $b) = ($a * $b) / arith1.gcd($a, $b)</ns1:popcorn>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/relation1#eq"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/arith1#lcm"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/arith1#divide"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/arith1#times"/>
      <ns1:usesSymbol rdf:resource="http://www.openmath.org/cd/arith1#gcd"/>
   </ns1:Property>
   <ns1:Property rdf:about="http://www.openmath.org/cd/arith1#lcm.prop2">
      <ns1:text>for all integers a,b | There does not exist a c&gt;0 such that c/a is an Integer and c/b is an Integer and lcm(a,b) &gt; c.</ns1:text>
      <ns1:openMath rdf:parseType="Literal">
         <OMOBJ xmlns="http://www.openmath.org/OpenMath"
                xmlns:rxr="http://ilrt.org/discovery/2004/03/rxr/"
                version="2.0"
                cdbase="http://www.openmath.org/cd">
            <OMBIND>
               <OMS cd="quant1" name="forall"/>
               <OMBVAR>
                  <OMV name="a"/>
                  <OMV name="b"/>
               </OMBVAR>
               <OMA>
                  <OMS cd="logic1" name="implies"/>
                  <OMA>
                     <OMS cd="logic1" name="and"/>
                     <OMA>
                        <OMS cd="set1" name="in"/>
                        <OMV name="a"/>
                        <OMS cd="setname1" name="Z"/>
                     </OMA>
                     <OMA>
                        <OMS cd="set1" name="in"/>
                        <OMV name="b"/>
                        <OMS cd="setname1" name="Z"/>
                     </OMA>
                  </OMA>
                  <OMA>
                     <OMS cd="logic1" name="not"/>
                     <OMBIND>
                        <OMS cd="quant1" name="exists"/>
                        <OMBVAR>
                           <OMV name="c"/>
                        </OMBVAR>
                        <OMA>
                           <OMS cd="logic1" name="and"/>
                           <OMA>
                              <OMS cd="relation1" name="gt"/>
                              <OMV name="c"/>
                              <OMI>0</OMI>
                           </OMA>
                           <OMA>
                              <OMS cd="integer1" name="factorof"/>
                              <OMV name="a"/>
                              <OMV name="c"/>
                           </OMA>
                           <OMA>
                              <OMS cd="integer1" name="factorof"/>
                              <OMV name="b"/>
                              <OMV name="c"/>
                           </OMA>
                           <OMA>
                              <OMS cd="relation1" name="lt"/>
                              <OMV name="c"/>
                              <OMA>
                                 <OMS cd="arith1" name="lcm"/>
                                 <OMV name="a"/>
                                 <OMV name="b"/>
                              </OMA>
                           </OMA>
                        </OMA>
                     </OMBIND>
                  </OMA>
               </OMA>
            </OMBIND>
         </OMOBJ>
      </ns1:openMath>
      <ns1:contentMathML rdf:parseType="Literal">
         <math xmlns="http://www.w3.org/1998/Math/MathML"
               xmlns:rxr="http://ilrt.org/discovery/2004/03/rxr/">
            <bind>
               <csymbol cd="quant1">forall</csymbol>
               <bvar>
                  <ci>a</ci>
               </bvar>
               <bvar>
                  <ci>b</ci>
               </bvar>
               <apply>
                  <csymbol cd="logic1">implies</csymbol>
                  <apply>
                     <csymbol cd="logic1">and</csymbol>
                     <apply>
                        <csymbol cd="set1">in</csymbol>
                        <ci>a</ci>
                        <csymbol cd="setname1">Z</csymbol>
                     </apply>
                     <apply>
                        <csymbol cd="set1">in</csymbol>
                        <ci>b</ci>
                        <csymbol cd="setname1">Z</csymbol>
                     </apply>
                  </apply>
                  <apply>
                     <csymbol cd="logic1">not</csymbol>
                     <bind>
                        <csymbol cd="quant1">exists</csymbol>
                        <bvar>
                           <ci>c</ci>
                        </bvar>
                        <apply>
                           <csymbol cd="logic1">and</csymbol>
                           <apply>
                              <csymbol cd="relation1">gt</csymbol>
                              <ci>c</ci>
                              <cn>0</cn>
                           </apply>
                           <apply>
                              <csymbol cd="integer1">factorof</csymbol>
                              <ci>a</ci>
                              <ci>c</ci>
                           </apply>
                           <apply>
                              <csymbol cd="integer1">factorof</csymbol>
                              <ci>b</ci>
                              <ci>c</ci>
                           </apply>
                           <apply>
                              <csymbol cd="relation1">lt</csymbol>
                              <ci>c</ci>
                              <apply>
                                 <csymbol cd="arith1">lcm</csymbol>
                                 <ci>a</ci>
                                 <ci>b</ci>
                              </apply>
                           </apply>
                        </apply>
                     </bind>
                  </apply>
               </apply>
            </bind>
         </math>
      </ns1:contentMathML>
      <ns1:popcorn>quant1.forall[$a, $b -&gt; set1.in($a, setname1.Z) and set1.in($b, setname1.Z) ==&gt;  not(quant1.exists[$c -&gt; $c &gt; 0 and integer1.factorof($a, $c) and integer1.factorof($b, $c) and $c &lt; arith1.lcm($a, $b)])]</ns1:popcorn>
