      <ns1:hasProperty rdf:resource="http://www.openmath.org/cd/arith1#unary_minus.prop0"/>
   </ns1:SymbolDefinition>
   <ns1:Property rdf:about="http://www.openmath.org/cd/arith1#unary_minus.prop0">
      <ns1:text>for all a | a + (-a) = 0</ns1:text>
      <ns1:openMath rdf:parseType="Literal">
         <OMOBJ xmlns="http://www.openmath.org/OpenMath"
                xmlns:rxr="http://ilrt.org/discovery/2004/03/rxr/"
                version="2.0"
                cdbase="http://www.openmath.org/cd">
            <OMBIND>
               <OMS cd="quant1" name="forall"/>
               <OMBVAR>
                  <OMV name="a"/>
