   <ns1:SymbolDefinition rdf:about="http://www.openmath.org/cd/alg1#zero">
      <ns3:identifier>zero</ns3:identifier>
      <ns1:role rdf:resource="http://www.openmath.org/ontology#Constant"/>
      <ns3:description>This symbol represents the additive identity element.</ns3:description>
      <ns1:hasProperty rdf:resource="http://www.openmath.org/cd/alg1#zero.prop0"/>
