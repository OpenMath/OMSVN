   </ns1:SymbolDefinition>
   <ns1:Property rdf:about="http://www.openmath.org/cd/alg1#zero.prop0">
      <ns1:text>for all a | a + 0 = a</ns1:text>
      <ns1:openMath rdf:parseType="Literal">
         <OMOBJ xmlns="http://www.openmath.org/OpenMath"
                xmlns:rxr="http://ilrt.org/discovery/2004/03/rxr/"
                version="2.0"
                cdbase="http://www.openmath.org/cd">
            <OMBIND>
               <OMS cd="quant1" name="forall"/>
               <OMBVAR>
                  <OMV name="a"/>
               </OMBVAR>
               <OMA>
                  <OMS cd="relation1" name="eq"/>
                  <OMA>
                     <OMS cd="arith1" name="plus"/>
                     <OMV name="a"/>
                     <OMS cd="alg1" name="zero"/>
                  </OMA>
                  <OMV name="a"/>
               </OMA>
            </OMBIND>
         </OMOBJ>
      </ns1:openMath>
      <ns1:contentMathML rdf:parseType="Literal">
         <math xmlns="http://www.w3.org/1998/Math/MathML"
               xmlns:rxr="http://ilrt.org/discovery/2004/03/rxr/">
            <bind>
               <csymbol cd="quant1">forall</csymbol>
               <bvar>
                  <ci>a</ci>
               </bvar>
               <apply>
                  <csymbol cd="relation1">eq</csymbol>
                  <apply>
                     <csymbol cd="arith1">plus</csymbol>
                     <ci>a</ci>
                     <csymbol cd="alg1">zero</csymbol>
                  </apply>
                  <ci>a</ci>
               </apply>
            </bind>
         </math>
      </ns1:contentMathML>
      <ns1:popcorn>quant1.forall[$a -&gt; $a + alg1.zero = $a]</ns1:popcorn>
