<?xml version="1.0"?>
<notations xmlns="http://omdoc.org/ns" xmlns:m="http://www.w3.org/1998/Math/MathML" xmlns:om="http://www.openmath.org/OpenMath" xmlns:o="http://omdoc.org/ns">
  <notation>
    <prototype>
      <om:OMS cd="quant3" name="forallin"/>
    </prototype>
    <prototype>
      <m:csymbol cd="quant3">forallin</m:csymbol>
    </prototype>
    <rendering>
      <m:mo>∀</m:mo>
    </rendering>
  </notation>

  <notation>
    <prototype>
      <om:OMBIND>
	<om:OMA>
	  <om:OMS cd="quant3" name="forallin" o:cr="quant"/>
	  <exprlist name="bvars"><expr name="set"/></exprlist>
	</om:OMA>
        <om:OMBVAR>
          <exprlist name="bvars"><expr name="bvar"/></exprlist>
        </om:OMBVAR>
        <expr name="scope"/>
      </om:OMBIND>
    </prototype>
    <prototype>
      <m:bind>
	<m:apply>
	  <m:csymbol cd="quant3" o:cr="quant">forallin</m:csymbol>
	  <exprlist name="bvars"><m:bvar><expr name="set"/></m:bvar></exprlist>
	</m:apply>
	<exprlist name="bvars"><m:bvar><expr name="bvar"/></m:bvar></exprlist>
        <expr name="scope"/>
      </m:bind>
    </prototype>
    <rendering>
      <m:mrow>
	<m:mo o:cr="quant">∀</m:mo>
	<m:mrow>
	  <iterate name="bvars">
	    <separator><m:mo o:egroup="separator" separator="true">,</m:mo></separator>
	    <m:mrow>
	      <o:render name="bvar"/>
	      <m:mo separator="true">:</m:mo>
	      <o:render name="set"/>
	    </m:mrow>
	  </iterate>
	</m:mrow>
	<m:mo separator="true">.</m:mo>
	<o:render name="scope"/>
      </m:mrow>
    </rendering>
  </notation>
  <notation>
