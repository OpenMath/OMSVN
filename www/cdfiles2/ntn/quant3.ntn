          <exprlist name="bvars"><expr name="bvar"/></exprlist>
        </om:OMBVAR>
        <expr name="scope"/>
      </om:OMBIND>
    </prototype>
    <prototype>
      <m:bind>
        <m:csymbol cd="quant3" o:cr="quant">forallin</m:csymbol>
	<exprlist name="bvars"><m:bvar><expr name="bvar"/></m:bvar></exprlist>
        <expr name="scope"/>
      </m:bind>
    </prototype>
    <rendering>
      <m:mrow>
	<m:mo o:cr="quant">∀</m:mo>
	<m:mrow>
	  <iterate name="bvars">
	    <separator><m:mo separator="true">,</m:mo></separator>
	    <o:render name="bvar"/>
	  </iterate>
	</m:mrow>
	<m:mo separator="true">.</m:mo>
	<o:render name="scope"/>
      </m:mrow>
    </rendering>
  </notation>
  

