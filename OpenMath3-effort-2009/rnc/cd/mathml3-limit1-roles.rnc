namespace m  ="http://www.w3.org/1998/Math/MathML"
limit_limit1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"limit1"},attribute name {"limit"}}
cd.application |= limit_limit1_elt
both_sides_limit1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"limit1"},attribute name {"both_sides"}}
cd.constant |= both_sides_limit1_elt
above_limit1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"limit1"},attribute name {"above"}}
cd.constant |= above_limit1_elt
below_limit1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"limit1"},attribute name {"below"}}
cd.constant |= below_limit1_elt
null_limit1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"limit1"},attribute name {"null"}}
cd.constant |= null_limit1_elt
tendsto_limit1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"limit1"},attribute name {"tendsto"}}
cd.application |= tendsto_limit1_elt
