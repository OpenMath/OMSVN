namespace m  ="http://www.w3.org/1998/Math/MathML"
piecewise_piece1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"piece1"},attribute name {"piecewise"}}
cd.application |= piecewise_piece1_elt
piece_piece1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"piece1"},attribute name {"piece"}}
cd.application |= piece_piece1_elt
otherwise_piece1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"piece1"},attribute name {"otherwise"}}
cd.application |= otherwise_piece1_elt
