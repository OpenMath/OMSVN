   <OMV name="Piece" />
 </OMA>
</OMOBJ>
</Signature>

<Signature name="otherwise" >
<OMOBJ>
 <OMA>
   <OMS name="mapsto" cd="sts" />
   <OMS name="Object" cd="sts" />
   <OMV name="Piece" />
 </OMA>
</OMOBJ>
</Signature>

</CDSignatures>
