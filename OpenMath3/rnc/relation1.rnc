namespace om = "http://www.openmath.org/OpenMath"
namespace m  ="http://www.w3.org/1998/Math/MathML"
eq_relation1_elt = element om:OMS {
        attribute cd {"relation1"},
        attribute name {"eq"}}
 |    element m:csymbol {
        attribute cd {"relation1"},
        attribute name {"eq"}}
lt_relation1_elt = element om:OMS {
        attribute cd {"relation1"},
        attribute name {"lt"}}
 |    element m:csymbol {
        attribute cd {"relation1"},
        attribute name {"lt"}}
gt_relation1_elt = element om:OMS {
        attribute cd {"relation1"},
        attribute name {"gt"}}
 |    element m:csymbol {
        attribute cd {"relation1"},
        attribute name {"gt"}}
neq_relation1_elt = element om:OMS {
        attribute cd {"relation1"},
        attribute name {"neq"}}
 |    element m:csymbol {
        attribute cd {"relation1"},
        attribute name {"neq"}}
leq_relation1_elt = element om:OMS {
        attribute cd {"relation1"},
        attribute name {"leq"}}
 |    element m:csymbol {
        attribute cd {"relation1"},
        attribute name {"leq"}}
geq_relation1_elt = element om:OMS {
        attribute cd {"relation1"},
        attribute name {"geq"}}
 |    element m:csymbol {
        attribute cd {"relation1"},
        attribute name {"geq"}}
approx_relation1_elt = element om:OMS {
        attribute cd {"relation1"},
        attribute name {"approx"}}
 |    element m:csymbol {
        attribute cd {"relation1"},
        attribute name {"approx"}}
Boolean_setname2_type |= element om:OMA {eq_relation1_elt,Object_sts_type,Object_sts_type}
Boolean_setname2_type |= element om:OMA {lt_relation1_elt,OrderedSet_type,OrderedSet_type}
Boolean_setname2_type |= element om:OMA {gt_relation1_elt,OrderedSet_type,OrderedSet_type}
Boolean_setname2_type |= element om:OMA {neq_relation1_elt,Object_sts_type,Object_sts_type}
Boolean_setname2_type |= element om:OMA {leq_relation1_elt,OrderedSet_type,OrderedSet_type}
Boolean_setname2_type |= element om:OMA {geq_relation1_elt,OrderedSet_type,OrderedSet_type}
Boolean_setname2_type |= element om:OMA {approx_relation1_elt,NumericalValue_sts_type,NumericalValue_sts_type}
