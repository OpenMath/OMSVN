namespace om = "http://www.openmath.org/OpenMath"
namespace m  ="http://www.w3.org/1998/Math/MathML"
integer_interval_interval1_elt = element om:OMS {
        attribute cd {"interval1"},
        attribute name {"integer_interval"}}
 |    element m:csymbol {
        attribute cd {"interval1"},
        attribute name {"integer_interval"}}
interval_interval1_elt = element om:OMS {
        attribute cd {"interval1"},
        attribute name {"interval"}}
 |    element m:csymbol {
        attribute cd {"interval1"},
        attribute name {"interval"}}
interval_oo_interval1_elt = element om:OMS {
        attribute cd {"interval1"},
        attribute name {"interval_oo"}}
 |    element m:csymbol {
        attribute cd {"interval1"},
        attribute name {"interval_oo"}}
interval_cc_interval1_elt = element om:OMS {
        attribute cd {"interval1"},
        attribute name {"interval_cc"}}
 |    element m:csymbol {
        attribute cd {"interval1"},
        attribute name {"interval_cc"}}
interval_oc_interval1_elt = element om:OMS {
        attribute cd {"interval1"},
        attribute name {"interval_oc"}}
 |    element m:csymbol {
        attribute cd {"interval1"},
        attribute name {"interval_oc"}}
interval_co_interval1_elt = element om:OMS {
        attribute cd {"interval1"},
        attribute name {"interval_co"}}
 |    element m:csymbol {
        attribute cd {"interval1"},
        attribute name {"interval_co"}}
IntegerInterval_type |= element om:OMA {integer_interval_interval1_elt,Z_setname1_type,Z_setname1_type}
RealInterval_type |= element om:OMA {interval_interval1_elt,R_setname1_type,R_setname1_type}
RealInterval_type |= element om:OMA {interval_oo_interval1_elt,R_setname1_type,R_setname1_type}
RealInterval_type |= element om:OMA {interval_cc_interval1_elt,R_setname1_type,R_setname1_type}
RealInterval_type |= element om:OMA {interval_oc_interval1_elt,R_setname1_type,R_setname1_type}
RealInterval_type |= element om:OMA {interval_co_interval1_elt,R_setname1_type,R_setname1_type}
