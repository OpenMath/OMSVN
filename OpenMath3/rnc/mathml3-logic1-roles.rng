      </attribute>
    </element>
  </define>
  <define name="cd.constant" combine="choice">
    <ref name="true_logic1_elt"/>
  </define>
  <define name="false_logic1_elt">
    <element name="m:csymbol">
      <ref name="MathML.Common.attrib"/>
      <attribute name="cd">
        <value>logic1</value>
      </attribute>
