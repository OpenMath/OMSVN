vector@linalg2 = element om:OMS {
        attribute cd {"linalg2"},
        attribute name {"vector"}}
 |    element m:csymbol {
        attribute cd {"linalg2"},
        attribute name {"vector"}}
matrix@linalg2 = element om:OMS {
        attribute cd {"linalg2"},
        attribute name {"matrix"}}
 |    element m:csymbol {
        attribute cd {"linalg2"},
        attribute name {"matrix"}}
matrixrow@linalg2 = element om:OMS {
        attribute cd {"linalg2"},
        attribute name {"matrixrow"}}
 |    element m:csymbol {
        attribute cd {"linalg2"},
        attribute name {"matrixrow"}}
Vector@omtypes | = element om:OMA {vector@linalg2,}
Matrix@omtypes | = element om:OMA {matrix@linalg2,}
MatrixRow@sts | = element om:OMA {matrixrow@linalg2,}
