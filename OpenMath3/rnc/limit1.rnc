limit@limit1 = element om:OMS {
        attribute cd {"limit1"},
        attribute name {"limit"}}
 |    element m:csymbol {
        attribute cd {"limit1"},
        attribute name {"limit"}}
both_sides@limit1 = element om:OMS {
        attribute cd {"limit1"},
        attribute name {"both_sides"}}
 |    element m:csymbol {
        attribute cd {"limit1"},
        attribute name {"both_sides"}}
above@limit1 = element om:OMS {
        attribute cd {"limit1"},
        attribute name {"above"}}
 |    element m:csymbol {
        attribute cd {"limit1"},
        attribute name {"above"}}
below@limit1 = element om:OMS {
        attribute cd {"limit1"},
        attribute name {"below"}}
 |    element m:csymbol {
        attribute cd {"limit1"},
        attribute name {"below"}}
null@limit1 = element om:OMS {
        attribute cd {"limit1"},
        attribute name {"null"}}
 |    element m:csymbol {
        attribute cd {"limit1"},
        attribute name {"null"}}
NumericalValue@sts | = element om:OMA {limit@limit1,NumericalValue@sts,TendsTo@omtypes,}
TendsTo | = both_sides@limit1
TendsTo | = above@limit1
TendsTo | = below@limit1
TendsTo | = null@limit1
