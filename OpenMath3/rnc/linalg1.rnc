CommutativeRing@sts | = element om:OMA {determinant@linalg1,SquareMatrix@omtypes}
Matrix@omtypes | = element om:OMA {transpose@linalg1,Matrix@omtypes}
Object@sts | = element om:OMA {vector_selector@linalg1,PositiveInteger@omtypes,Object@sts}
Object@sts | = element om:OMA {matrix_selector@linalg1,PositiveInteger@omtypes,PositiveInteger@omtypes,Matrix@omtypes}
ThreeVector@omtypes | = element om:OMA {vectorproduct@linalg1,ThreeVector@omtypes,ThreeVector@omtypes}
Scalar@omtypes | = element om:OMA {scalarproduct@linalg1,Vector@omtypes,Vector@omtypes}
Matrix@omtypes | = element om:OMA {outerproduct@linalg1,Vector@omtypes,Vector@omtypes}
