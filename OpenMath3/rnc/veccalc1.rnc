divergence@veccalc1 = element om:OMS {
        attribute cd {"veccalc1"},
        attribute name {"divergence"}}
 |    element m:csymbol {
        attribute cd {"veccalc1"},
        attribute name {"divergence"}}
grad@veccalc1 = element om:OMS {
        attribute cd {"veccalc1"},
        attribute name {"grad"}}
 |    element m:csymbol {
        attribute cd {"veccalc1"},
        attribute name {"grad"}}
curl@veccalc1 = element om:OMS {
        attribute cd {"veccalc1"},
        attribute name {"curl"}}
 |    element m:csymbol {
        attribute cd {"veccalc1"},
        attribute name {"curl"}}
Laplacian@veccalc1 = element om:OMS {
        attribute cd {"veccalc1"},
        attribute name {"Laplacian"}}
 |    element m:csymbol {
        attribute cd {"veccalc1"},
        attribute name {"Laplacian"}}
 | = element om:OMA {divergence@veccalc1,}
 | = element om:OMA {grad@veccalc1,}
 | = element om:OMA {curl@veccalc1,}
 | = element om:OMA {Laplacian@veccalc1,}
