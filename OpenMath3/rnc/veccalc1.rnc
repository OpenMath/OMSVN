namespace om = "http://www.openmath.org/OpenMath"
namespace m  ="http://www.w3.org/1998/Math/MathML"
divergence_veccalc1_elt = element om:OMS {
        attribute cd {"veccalc1"},
        attribute name {"divergence"}}
 |    element m:csymbol {
        attribute cd {"veccalc1"},
        attribute name {"divergence"}}
grad_veccalc1_elt = element om:OMS {
        attribute cd {"veccalc1"},
        attribute name {"grad"}}
 |    element m:csymbol {
        attribute cd {"veccalc1"},
        attribute name {"grad"}}
curl_veccalc1_elt = element om:OMS {
        attribute cd {"veccalc1"},
        attribute name {"curl"}}
 |    element m:csymbol {
        attribute cd {"veccalc1"},
        attribute name {"curl"}}
Laplacian_veccalc1_elt = element om:OMS {
        attribute cd {"veccalc1"},
        attribute name {"Laplacian"}}
 |    element m:csymbol {
        attribute cd {"veccalc1"},
        attribute name {"Laplacian"}}
function_type |= element om:OMA {divergence_veccalc1_elt,function_type}
function_type |= element om:OMA {grad_veccalc1_elt,function_type}
function_type |= element om:OMA {curl_veccalc1_elt,ThreeVector_type}
function_type |= element om:OMA {Laplacian_veccalc1_elt,function_type}
