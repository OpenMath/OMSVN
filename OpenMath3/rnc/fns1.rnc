identity@fns1 = element om:OMS {
        attribute cd {"fns1"},
        attribute name {"identity"}}
 |    element m:csymbol {
        attribute cd {"fns1"},
        attribute name {"identity"}}
left_inverse@fns1 = element om:OMS {
        attribute cd {"fns1"},
        attribute name {"left_inverse"}}
 |    element m:csymbol {
        attribute cd {"fns1"},
        attribute name {"left_inverse"}}
right_inverse@fns1 = element om:OMS {
        attribute cd {"fns1"},
        attribute name {"right_inverse"}}
 |    element m:csymbol {
        attribute cd {"fns1"},
        attribute name {"right_inverse"}}
inverse@fns1 = element om:OMS {
        attribute cd {"fns1"},
        attribute name {"inverse"}}
 |    element m:csymbol {
        attribute cd {"fns1"},
        attribute name {"inverse"}}
domain@fns1 = element om:OMS {
        attribute cd {"fns1"},
        attribute name {"domain"}}
 |    element m:csymbol {
        attribute cd {"fns1"},
        attribute name {"domain"}}
domainofapplication@fns1 = element om:OMS {
        attribute cd {"fns1"},
        attribute name {"domainofapplication"}}
 |    element m:csymbol {
        attribute cd {"fns1"},
        attribute name {"domainofapplication"}}
range@fns1 = element om:OMS {
        attribute cd {"fns1"},
        attribute name {"range"}}
 |    element m:csymbol {
        attribute cd {"fns1"},
        attribute name {"range"}}
left_compose@fns1 = element om:OMS {
        attribute cd {"fns1"},
        attribute name {"left_compose"}}
 |    element m:csymbol {
        attribute cd {"fns1"},
        attribute name {"left_compose"}}
lambda@fns1 = element om:OMS {
        attribute cd {"fns1"},
        attribute name {"lambda"}}
 |    element m:csymbol {
        attribute cd {"fns1"},
        attribute name {"lambda"}}
Object@sts | = element om:OMA {identity@fns1,Object@sts}
 | = element om:OMA {left_inverse@fns1,}
 | = element om:OMA {right_inverse@fns1,}
 | = element om:OMA {inverse@fns1,}
Set@omtypes | = element om:OMA {domain@fns1,}
Set@omtypes | = element om:OMA {domainofapplication@fns1,}
Set@omtypes | = element om:OMA {range@fns1,}
 | = element om:OMA {left_compose@fns1,,}
binder | = lambda@fns1
