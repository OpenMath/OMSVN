factorof@integer1 = element om:OMS {
        attribute cd {"integer1"},
        attribute name {"factorof"}}
 |    element m:csymbol {
        attribute cd {"integer1"},
        attribute name {"factorof"}}
factorial@integer1 = element om:OMS {
        attribute cd {"integer1"},
        attribute name {"factorial"}}
 |    element m:csymbol {
        attribute cd {"integer1"},
        attribute name {"factorial"}}
quotient@integer1 = element om:OMS {
        attribute cd {"integer1"},
        attribute name {"quotient"}}
 |    element m:csymbol {
        attribute cd {"integer1"},
        attribute name {"quotient"}}
remainder@integer1 = element om:OMS {
        attribute cd {"integer1"},
        attribute name {"remainder"}}
 |    element m:csymbol {
        attribute cd {"integer1"},
        attribute name {"remainder"}}
N@setname1 | = element om:OMA {factorial@integer1,N@setname1}
Z@setname1 | = element om:OMA {quotient@integer1,Z@setname1,Z@setname1}
Z@setname1 | = element om:OMA {remainder@integer1,Z@setname1,Z@setname1}
