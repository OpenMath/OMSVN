conjugate@complex1 = element om:OMS {
        attribute cd {"complex1"},
        attribute name {"conjugate"}}
 |    element m:csymbol {
        attribute cd {"complex1"},
        attribute name {"conjugate"}}
complex_cartesian@complex1 = element om:OMS {
        attribute cd {"complex1"},
        attribute name {"complex_cartesian"}}
 |    element m:csymbol {
        attribute cd {"complex1"},
        attribute name {"complex_cartesian"}}
real@complex1 = element om:OMS {
        attribute cd {"complex1"},
        attribute name {"real"}}
 |    element m:csymbol {
        attribute cd {"complex1"},
        attribute name {"real"}}
imaginary@complex1 = element om:OMS {
        attribute cd {"complex1"},
        attribute name {"imaginary"}}
 |    element m:csymbol {
        attribute cd {"complex1"},
        attribute name {"imaginary"}}
complex_polar@complex1 = element om:OMS {
        attribute cd {"complex1"},
        attribute name {"complex_polar"}}
 |    element m:csymbol {
        attribute cd {"complex1"},
        attribute name {"complex_polar"}}
argument@complex1 = element om:OMS {
        attribute cd {"complex1"},
        attribute name {"argument"}}
 |    element m:csymbol {
        attribute cd {"complex1"},
        attribute name {"argument"}}
C@setname1 | = element om:OMA {conjugate@complex1,C@setname1}
C@setname1 | = element om:OMA {complex_cartesian@complex1,NumericalValue@sts,NumericalValue@sts}
NumericalValue@sts | = element om:OMA {real@complex1,C@setname1}
NumericalValue@sts | = element om:OMA {imaginary@complex1,C@setname1}
C@setname1 | = element om:OMA {complex_polar@complex1,PositiveReal@omtypes,R@setname1}
R@setname1 | = element om:OMA {argument@complex1,C@setname1}
