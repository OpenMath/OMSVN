type@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"type"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"type"}}
mapsto@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"mapsto"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"mapsto"}}
nary@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"nary"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"nary"}}
nassoc@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"nassoc"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"nassoc"}}
error@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"error"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"error"}}
structure@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"structure"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"structure"}}
binder@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"binder"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"binder"}}
attribution@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"attribution"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"attribution"}}
Object@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"Object"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"Object"}}
NumericalValue@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"NumericalValue"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"NumericalValue"}}
SetNumericalValue@sts = element om:OMS {
        attribute cd {"sts"},
        attribute name {"SetNumericalValue"}}
 |    element m:csymbol {
        attribute cd {"sts"},
        attribute name {"SetNumericalValue"}}
attribution | = type@sts
FunctionType@omtypes | = element om:OMA {mapsto@sts,}
Tuple@omtypes | = element om:OMA {nary@sts,Type@omtypes}
Tuple@omtypes | = element om:OMA {nassoc@sts,Type@omtypes}
Error@omtypes | = element om:OMA {error@sts,String@omtypes}
Domain@omtypes | = element om:OMA {structure@sts,Category@omtypes}
Symbol | = binder@sts
Symbol | = attribution@sts
Symbol | = Object@sts
Symbol | = NumericalValue@sts
Symbol | = SetNumericalValue@sts
