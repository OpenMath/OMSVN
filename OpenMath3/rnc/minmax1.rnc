namespace om = "http://www.openmath.org/OpenMath"
namespace m  ="http://www.w3.org/1998/Math/MathML"
min_minmax1_elt = element om:OMS {
        attribute cd {"minmax1"},
        attribute name {"min"}}
 |    element m:csymbol {
        attribute cd {"minmax1"},
        attribute name {"min"}}
max_minmax1_elt = element om:OMS {
        attribute cd {"minmax1"},
        attribute name {"max"}}
 |    element m:csymbol {
        attribute cd {"minmax1"},
        attribute name {"max"}}
OrderedSet_type |= element om:OMA {min_minmax1_elt,OrderedSet_type}
OrderedSet_type |= element m:apply {min_minmax1_elt,OrderedSet_type}
OrderedSet_type |= element om:OMA {max_minmax1_elt,OrderedSet_type}
OrderedSet_type |= element m:apply {max_minmax1_elt,OrderedSet_type}
