min@minmax1 = element om:OMS {
        attribute cd {"minmax1"},
        attribute name {"min"}}
 |    element m:csymbol {
        attribute cd {"minmax1"},
        attribute name {"min"}}
max@minmax1 = element om:OMS {
        attribute cd {"minmax1"},
        attribute name {"max"}}
 |    element m:csymbol {
        attribute cd {"minmax1"},
        attribute name {"max"}}
OrderedSet@omtypes | = element om:OMA {min@minmax1,}
OrderedSet@omtypes | = element om:OMA {max@minmax1,}
