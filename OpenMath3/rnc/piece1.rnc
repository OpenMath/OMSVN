piecewise@piece1 = element om:OMS {
        attribute cd {"piece1"},
        attribute name {"piecewise"}}
 |    element m:csymbol {
        attribute cd {"piece1"},
        attribute name {"piecewise"}}
piece@piece1 = element om:OMS {
        attribute cd {"piece1"},
        attribute name {"piece"}}
 |    element m:csymbol {
        attribute cd {"piece1"},
        attribute name {"piece"}}
otherwise@piece1 = element om:OMS {
        attribute cd {"piece1"},
        attribute name {"otherwise"}}
 |    element m:csymbol {
        attribute cd {"piece1"},
        attribute name {"otherwise"}}
Object@sts | = element om:OMA {piecewise@piece1,}
Piece@omtypes | = element om:OMA {piece@piece1,Object@sts,Boolean@setname2}
Piece@omtypes | = element om:OMA {otherwise@piece1,Object@sts}
