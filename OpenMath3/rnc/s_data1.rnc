mean@s_data1 = element om:OMS {
        attribute cd {"s_data1"},
        attribute name {"mean"}}
 |    element m:csymbol {
        attribute cd {"s_data1"},
        attribute name {"mean"}}
sdev@s_data1 = element om:OMS {
