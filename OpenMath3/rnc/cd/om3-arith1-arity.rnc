namespace om = "http://www.openmath.org/OpenMath"

lcm_arith1_elt = element om:OMS{attribute cd {"arith1"}, attribute name {"lcm"}}
cd.nary |= lcm_arith1_elt
big_lcm_arith1_elt = element om:OMS{attribute cd {"arith1"}, attribute name {"big_lcm"}}
cd.binders |= big_lcm_arith1_elt
gcd_arith1_elt = element om:OMS{attribute cd {"arith1"}, attribute name {"gcd"}}
cd.nary |= gcd_arith1_elt
big_gcd_arith1_elt = element om:OMS{attribute cd {"arith1"}, attribute name {"big_gcd"}}
cd.binders |= big_gcd_arith1_elt
