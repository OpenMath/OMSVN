namespace m  ="http://www.w3.org/1998/Math/MathML"
vector_linalg3_elt = element m:csymbol {
        attribute cd {"linalg3"},
        attribute name {"vector"}}
matrix_linalg3_elt = element m:csymbol {
        attribute cd {"linalg3"},
        attribute name {"matrix"}}
matrixcolumn_linalg3_elt = element m:csymbol {
        attribute cd {"linalg3"},
        attribute name {"matrixcolumn"}}
Vector_type |= element m:apply {vector_linalg3_elt,AbelianGroupMult_type*}
Matrix_type |= element m:apply {matrix_linalg3_elt,MatrixRow_type*}
MatrixRow_type |= element m:apply {matrixcolumn_linalg3_elt,Ring_type*}
