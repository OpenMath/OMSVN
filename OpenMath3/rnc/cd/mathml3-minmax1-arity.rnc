namespace m  ="http://www.w3.org/1998/Math/MathML"
min_minmax1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"minmax1"},attribute name {"min"}}
cd.unary |= min_minmax1_elt
big_min_minmax1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"minmax1"},attribute name {"big_min"}}
cd.binders |= big_min_minmax1_elt
max_minmax1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"minmax1"},attribute name {"max"}}
cd.unary |= max_minmax1_elt
big_max_minmax1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"minmax1"},attribute name {"big_max"}}
cd.binders |= big_max_minmax1_elt
