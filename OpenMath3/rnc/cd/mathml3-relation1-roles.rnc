namespace m  ="http://www.w3.org/1998/Math/MathML"
eq_relation1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"relation1"},attribute name {"eq"}}
cd.application |= eq_relation1_elt
lt_relation1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"relation1"},attribute name {"lt"}}
cd.application |= lt_relation1_elt
gt_relation1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"relation1"},attribute name {"gt"}}
cd.application |= gt_relation1_elt
neq_relation1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"relation1"},attribute name {"neq"}}
cd.application |= neq_relation1_elt
leq_relation1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"relation1"},attribute name {"leq"}}
cd.application |= leq_relation1_elt
geq_relation1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"relation1"},attribute name {"geq"}}
cd.application |= geq_relation1_elt
approx_relation1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"relation1"},attribute name {"approx"}}
cd.application |= approx_relation1_elt
