;

namespace om = "http://www.openmath.org/OpenMath"

unhandled_symbol_error_elt = element om:OMS {
        attribute cd {"error"},
        attribute name {"unhandled_symbol"}}
unexpected_symbol_error_elt = element om:OMS {
        attribute cd {"error"},
        attribute name {"unexpected_symbol"}}
unsupported_CD_error_elt = element om:OMS {
        attribute cd {"error"},
        attribute name {"unsupported_CD"}}
error_sts_type |= element om:OMA {unhandled_symbol_error_elt,OMSymbol_omtypes_type}
error_sts_type |= element om:OMA {unexpected_symbol_error_elt,OMSymbol_omtypes_type}
error_sts_type |= element om:OMA {unsupported_CD_error_elt,OMSymbol_omtypes_type}
