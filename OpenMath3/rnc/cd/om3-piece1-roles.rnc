namespace om = "http://www.openmath.org/OpenMath"

piecewise_piece1_elt = element om:OMS {common.attributes,cdbase,attribute cd {"piece1"},attribute name {"piecewise"}}
cd.application |= piecewise_piece1_elt
piece_piece1_elt = element om:OMS {common.attributes,cdbase,attribute cd {"piece1"},attribute name {"piece"}}
cd.application |= piece_piece1_elt
otherwise_piece1_elt = element om:OMS {common.attributes,cdbase,attribute cd {"piece1"},attribute name {"otherwise"}}
cd.application |= otherwise_piece1_elt
