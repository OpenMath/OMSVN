namespace om = "http://www.openmath.org/OpenMath"vector_linalg2_elt = element om:OMS {
        attribute cd {"linalg2"},
        attribute name {"vector"}}
 |    element m:csymbol {
        attribute cd {"linalg2"},
        attribute name {"vector"}}
matrix_linalg2_elt = element om:OMS {
        attribute cd {"linalg2"},
        attribute name {"matrix"}}
 |    element m:csymbol {
        attribute cd {"linalg2"},
        attribute name {"matrix"}}
matrixrow_linalg2_elt = element om:OMS {
        attribute cd {"linalg2"},
        attribute name {"matrixrow"}}
 |    element m:csymbol {
        attribute cd {"linalg2"},
        attribute name {"matrixrow"}}
Vector_type |= element om:OMA {vector_linalg2_elt,AbelianGroupMult_type*}
Matrix_type |= element om:OMA {matrix_linalg2_elt,MatrixRow_type*}
MatrixRow_type |= element om:OMA {matrixrow_linalg2_elt,Ring_type*}
