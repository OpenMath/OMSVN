namespace m  ="http://www.w3.org/1998/Math/MathML"
diff_calculus1_elt = element m:csymbol {
        attribute cd {"calculus1"},
        attribute name {"diff"}}
nthdiff_calculus1_elt = element m:csymbol {
        attribute cd {"calculus1"},
        attribute name {"nthdiff"}}
partialdiff_calculus1_elt = element m:csymbol {
        attribute cd {"calculus1"},
        attribute name {"partialdiff"}}
int_calculus1_elt = element m:csymbol {
        attribute cd {"calculus1"},
        attribute name {"int"}}
defint_calculus1_elt = element m:csymbol {
        attribute cd {"calculus1"},
        attribute name {"defint"}}
function_type |= element m:apply {diff_calculus1_elt,function_type}
function_type |= element m:apply {nthdiff_calculus1_elt,Z_setname1_type,function_type}
function_type |= element m:apply {partialdiff_calculus1_elt,VariableList_type,function_type}
function_type |= element m:apply {int_calculus1_elt,function_type}
Object_sts_type |= element m:apply {defint_calculus1_elt,RangeOfIntegration_type,function_type}
