namespace om = "http://www.openmath.org/OpenMath"

piecewise_piece1_elt = element om:OMS{attribute cd {"piece1"}, attribute name {"piecewise"}}
cd.unary |= piecewise_piece1_elt
piece_piece1_elt = element om:OMS{attribute cd {"piece1"}, attribute name {"piece"}}
cd.binary |= piece_piece1_elt
otherwise_piece1_elt = element om:OMS{attribute cd {"piece1"}, attribute name {"otherwise"}}
cd.unary |= otherwise_piece1_elt
