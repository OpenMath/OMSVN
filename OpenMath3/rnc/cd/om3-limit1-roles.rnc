namespace om = "http://www.openmath.org/OpenMath"

limit_limit1_elt = element om:OMS {common.attributes,cdbase,attribute cd {"limit1"},attribute name {"limit"}}
cd.application |= limit_limit1_elt
both_sides_limit1_elt = element om:OMS {common.attributes,cdbase,attribute cd {"limit1"},attribute name {"both_sides"}}
cd.constant |= both_sides_limit1_elt
above_limit1_elt = element om:OMS {common.attributes,cdbase,attribute cd {"limit1"},attribute name {"above"}}
cd.constant |= above_limit1_elt
below_limit1_elt = element om:OMS {common.attributes,cdbase,attribute cd {"limit1"},attribute name {"below"}}
cd.constant |= below_limit1_elt
null_limit1_elt = element om:OMS {common.attributes,cdbase,attribute cd {"limit1"},attribute name {"null"}}
cd.constant |= null_limit1_elt
