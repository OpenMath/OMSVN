namespace om = "http://www.openmath.org/OpenMath"

piecewise_piece1_elt = element om:OMS {
        attribute cd {"piece1"},
        attribute name {"piecewise"}}
piece_piece1_elt = element om:OMS {
        attribute cd {"piece1"},
        attribute name {"piece"}}
otherwise_piece1_elt = element om:OMS {
        attribute cd {"piece1"},
        attribute name {"otherwise"}}
Object_sts_type |= element om:OMA {piecewise_piece1_elt,Piece_type*}
Piece_type |= element om:OMA {piece_piece1_elt,Object_sts_type,Boolean_setname2_type}
Piece_type |= element om:OMA {otherwise_piece1_elt,Object_sts_type}
