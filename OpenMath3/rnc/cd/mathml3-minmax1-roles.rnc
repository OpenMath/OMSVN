namespace m  ="http://www.w3.org/1998/Math/MathML"
min_minmax1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"minmax1"},attribute name {"min"}}
cd.application |= min_minmax1_elt
max_minmax1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"minmax1"},attribute name {"max"}}
cd.application |= max_minmax1_elt
