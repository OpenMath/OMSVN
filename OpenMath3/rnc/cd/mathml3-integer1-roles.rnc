namespace m  ="http://www.w3.org/1998/Math/MathML"
factorof_integer1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"integer1"},attribute name {"factorof"}}
cd.application |= factorof_integer1_elt
factorial_integer1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"integer1"},attribute name {"factorial"}}
cd.application |= factorial_integer1_elt
quotient_integer1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"integer1"},attribute name {"quotient"}}
cd.application |= quotient_integer1_elt
remainder_integer1_elt = element m:csymbol {MathML.Common.attrib,attribute cd {"integer1"},attribute name {"remainder"}}
cd.application |= remainder_integer1_elt
