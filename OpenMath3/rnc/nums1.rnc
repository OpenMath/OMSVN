based_integer@nums1 = element om:OMS {
        attribute cd {"nums1"},
        attribute name {"based_integer"}}
 |    element m:csymbol {
        attribute cd {"nums1"},
        attribute name {"based_integer"}}
rational@nums1 = element om:OMS {
