forall@quant1 = element om:OMS {
        attribute cd {"quant1"},
        attribute name {"forall"}}
 |    element m:csymbol {
        attribute cd {"quant1"},
        attribute name {"forall"}}
exists@quant1 = element om:OMS {
        attribute cd {"quant1"},
        attribute name {"exists"}}
 |    element m:csymbol {
        attribute cd {"quant1"},
        attribute name {"exists"}}
binder | = forall@quant1
binder | = exists@quant1
