<CDDefinition xml:id="approx">
  <Name>approx</Name>
  <Pragmatic>
    <Element type="opel">approx</Element>
    <Attribute>
      <Name>type</Name>
      <Model>MathMLType</Model>
      <Default>real</Default>
    </Attribute>
  </Pragmatic>
  <Role>application</Role>
  <Description> 
    This symbol is used to denote the approximate equality of its two arguments.
  </Description>

  <property>
    <description>
      <p> Symmetric</p>
    </description>
  </property>

  <property>
    <description>
      <p>Transitive</p>
    </description>
  </property>

  <property>
    <description>
      <p>Reflexive</p>
    </description>
  </property>
  
  <MMLexample speclevel="1" xml:id="approx.ex1">
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <apply><approx/><pi/><cn type="rational">22<sep/>7</cn></apply>
    </math>
  </MMLexample>
     

<Example>
\pi is approximately 355/113

<OMOBJ xmlns="http://www.openmath.org/OpenMath" version="3.0" cdbase="http://www.openmath.org/cd">
  <OMA>
    <OMS cd="relation1" name="approx"/>
    <OMS cd="nums1" name="pi"/>
    <OMA>
      <OMS cd="nums1" name="rational"/>
      <OMI> 355 </OMI>
      <OMI> 113 </OMI>
    </OMA>
  </OMA>
</OMOBJ>
</Example>
</CDDefinition>

