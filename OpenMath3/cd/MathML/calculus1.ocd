  <CDDefinition xml:id="defint">
    <Name/>
    <Pragmatic>
      <Element type="opel">defint</Element>
    </Pragmatic>
    <Role/>
    <Description/>
  
    <description>
      <p>The definite integral of a function with upper and lower bounds.</p>
    </description>

    <MMLexample>
      <description>
	       <p>This example specifies an interval of the real line as the domain of
	integration with an <el>interval</el> element.  In this form the integrand is
	provided as a function and no mention is made of a bound variable.</p>
      </description>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
	       <apply>
	         <defint/>
	         <apply>
            <interval/>
            <ci>a</ci>
            <ci>b</ci>
          </apply>
	         <cos/>
	       </apply>
      </math>
    </MMLexample>
    
    <property xml:id="defint_prop_6">
      <CMP> for all a,b | integral from a to b = -integral from b to a </CMP>

      <FMP>
        <OMOBJ xmlns="http://www.openmath.org/OpenMath" cdbase="http://www.openmath.org/cd"
               version="3.0">
          <OMBIND>
            <OMS cd="quant1" name="forall"/>
            <OMBVAR>
              <OMV name="a"/>
              <OMV name="b"/>
            </OMBVAR>
            <OMA>
              <OMS cd="relation1" name="eq"/>
              <OMA>
                <OMS cd="calculus1" name="defint"/>
                <OMA>
                  <OMS cd="interval1" name="interval"/>
                  <OMV name="a"/>
                  <OMV name="b"/>
                </OMA>
                <OMBIND>
                  <OMS cd="fns1" name="lambda"/>
                  <OMBVAR>
                    <OMV name="x"/>
                  </OMBVAR>
                  <OMA>
                    <OMV name="f"/>
                    <OMV name="x"/>
                  </OMA>
                </OMBIND>
              </OMA>
              <OMA>
                <OMS cd="arith1" name="unary_minus"/>
                <OMA>
                  <OMS cd="calculus1" name="defint"/>
                  <OMA>
                    <OMS cd="interval1" name="interval"/>
                    <OMV name="b"/>
                    <OMV name="a"/>
                  </OMA>
                  <OMBIND>
                    <OMS cd="fns1" name="lambda"/>
                    <OMBVAR>
                      <OMV name="x"/>
                    </OMBVAR>
                    <OMA>
                      <OMV name="f"/>
                      <OMV name="x"/>
                    </OMA>
                  </OMBIND>
                </OMA>
              </OMA>
            </OMA>
          </OMBIND>
        </OMOBJ>
      </FMP>
    </property>

    <property xml:id="defint_prop_7">
      <CMP>
for all a &lt; b &lt; c | integral over [a,c] = integral over [a,b] + 
integral over [b,c]
</CMP>

      <FMP>
        <OMOBJ xmlns="http://www.openmath.org/OpenMath" cdbase="http://www.openmath.org/cd"
               version="3.0">
          <OMBIND>
            <OMS cd="quant1" name="forall"/>
            <OMBVAR>
              <OMV name="a"/>
              <OMV name="b"/>
              <OMV name="c"/>
            </OMBVAR>
            <OMA>
              <OMS cd="logic1" name="implies"/>
              <OMA>
                <OMS cd="logic1" name="and"/>
                <OMA>
                  <OMS cd="relation1" name="lt"/>
                  <OMV name="a"/>
  	               <OMV name="b"/>
                </OMA>
                <OMA>
                  <OMS cd="relation1" name="lt"/>
	                 <OMV name="b"/>
	                 <OMV name="c"/>
                </OMA>
              </OMA>
              <OMA>
                <OMS cd="relation1" name="eq"/>
	               <OMA>
	                 <OMS cd="calculus1" name="defint"/>
	                 <OMA>
	                   <OMS cd="interval1" name="interval"/>
	                   <OMV name="a"/>
	                   <OMV name="c"/>
	                 </OMA>
	                 <OMBIND>
	                   <OMS cd="fns1" name="lambda"/>
	                   <OMBVAR>
	                     <OMV name="x"/>
	                   </OMBVAR>
	                   <OMA>
	                     <OMV name="f"/>
	                     <OMV name="x"/>
