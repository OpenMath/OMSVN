  </m:apply></m:bind>
</m:math>
      </mcd:formal>
    </mcd:property>
  </mcd:MMLDefinition>
  <mcd:MMLDefinition xml:id="" cdrole="application">
    <mcd:property>
      <mcd:description>
        <p> whenever not(a=0) then a/a = 1 </p>
      </mcd:description>
      <mcd:formal>
<m:math xmlns:m="http://www.w3.org/1998/Math/MathML" version="2.0" cdbase="http://www.openmath.org/cd">
