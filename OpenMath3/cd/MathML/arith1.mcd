          </m:apply>
          <m:apply>
            <m:csymbol cd="integer1" name="factorof"/>
              <m:ci>a</m:ci>
              <m:ci>c</m:ci>
          </m:apply>
          <m:apply>
            <m:csymbol cd="integer1" name="factorof"/>
              <m:ci>b</m:ci>
              <m:ci>c</m:ci>
          </m:apply>
          <m:apply>
            <m:csymbol cd="relation1" name="lt"/>
            <m:ci>c</m:ci>
            <m:apply>
              <m:csymbol cd="arith1" name="lcm"/>
              <m:ci>a</m:ci>
              <m:ci>b</m:ci>
            </m:apply>
          </m:apply>
        </m:apply></m:bind>
    </m:apply>
  </m:apply></m:bind>
</m:math>
