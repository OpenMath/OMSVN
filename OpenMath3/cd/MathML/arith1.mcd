  <mcd:MMLDefinition xml:id="" cdrole="application">
    <mcd:property>
      <mcd:description>
        <p> for all a,b | a * 0 = 0 and a * b = a * (b - 1) + a </p>
      </mcd:description>
      <mcd:formal>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML" version="2.0" cdbase="http://www.openmath.org/cd">
<m:bind><m:csymbol cd="quant1" name="forall"/><m:bvar>
    <m:ci>a</m:ci>
    <m:ci>b</m:ci>
  </m:bvar><m:apply>
    <m:csymbol cd="logic1" name="and"/>
    <m:apply>
      <m:csymbol cd="relation1" name="eq"/>
      <m:apply>
        <m:csymbol cd="arith1" name="times"/>
        <m:ci>a</m:ci>
        <m:csymbol cd="alg1" name="zero"/>
      </m:apply>
      <m:csymbol cd="alg1" name="zero"/>
    </m:apply>
    <m:apply>
      <m:csymbol cd="relation1" name="eq"/>
      <m:apply>
        <m:csymbol cd="arith1" name="times"/>
	<m:ci>a</m:ci>
	<m:ci>b</m:ci>
      </m:apply>
      <m:apply>
        <m:csymbol cd="arith1" name="plus"/>
	<m:apply>
	  <m:csymbol cd="arith1" name="times"/>
	  <m:ci>a</m:ci>
	  <m:apply>
	    <m:csymbol cd="arith1" name="minus"/>
	    <m:ci>b</m:ci>
	    <m:csymbol cd="alg1" name="one"/>
	  </m:apply>
	</m:apply>
	<m:ci>a</m:ci>
      </m:apply>
    </m:apply>
  </m:apply></m:bind>
</m:math>
      </mcd:formal>
    </mcd:property>
    <mcd:property>
      <mcd:description>
        <p> for all a,b,c | a*(b+c) = a*b + a*c </p>
