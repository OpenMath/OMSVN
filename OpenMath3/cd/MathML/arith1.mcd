    <m:csymbol cd="relation1" name="eq"/>
    <m:apply>
      <m:csymbol cd="arith1" name="lcm"/>
      <m:ci>a</m:ci>
      <m:ci>b</m:ci>
    </m:apply>
    <m:apply>
      <m:csymbol cd="arith1" name="divide"/>
      <m:apply>
        <m:csymbol cd="arith1" name="times"/>
	<m:ci>a</m:ci>
	<m:ci>b</m:ci>
      </m:apply>
      <m:apply>
        <m:csymbol cd="arith1" name="gcd"/>
        <m:ci>a</m:ci>
        <m:ci>b</m:ci>
      </m:apply>
    </m:apply>
  </m:apply>
</m:math>
</mcd:formal>
    </mcd:property>
    <mcd:property>
      <mcd:description>
        <p>
