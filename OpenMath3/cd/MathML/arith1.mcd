  <mcd:MMLDefinition xml:id="" cdrole="application">
    <mcd:property>
      <mcd:description>
        <p>
x\in C implies x^a = exp(a ln x)
</p>
      </mcd:description>
      <mcd:formal>
<m:math xmlns:m="http://www.w3.org/1998/Math/MathML" version="2.0" cdbase="http://www.openmath.org/cd">
  <m:apply>
    <m:csymbol cd="logic1" name="implies"/>
    <m:apply>
      <m:csymbol cd="set1" name="in"/>
      <m:ci>x</m:ci>
      <m:csymbol cd="setname1" name="C"/>
    </m:apply>
    <m:apply>
      <m:csymbol cd="relation1" name="eq"/>
      <m:apply>
        <m:csymbol name="power" cd="arith1"/>
        <m:ci>x</m:ci>
        <m:ci>a</m:ci>
      </m:apply>
      <m:apply>
        <m:csymbol name="exp" cd="transc1"/>
        <m:apply>
          <m:csymbol name="times" cd="arith1"/>
          <m:ci>a</m:ci>
          <m:apply>
            <m:csymbol name="ln" cd="transc1"/>
            <m:ci>x</m:ci>
          </m:apply>
        </m:apply>
      </m:apply>
    </m:apply>
  </m:apply>
</m:math>
</mcd:formal>
    </mcd:property>
    <mcd:property>
      <mcd:description>
        <p>
  if n is an integer then
  x^0 = 1,
  x^n = x * x^(n-1)
</p>
      </mcd:description>
      <mcd:formal>
<m:math xmlns:m="http://www.w3.org/1998/Math/MathML" version="2.0" cdbase="http://www.openmath.org/cd">
  <m:apply>
    <m:csymbol cd="logic1" name="implies"/>
    <m:apply>
      <m:csymbol cd="set1" name="in"/>
      <m:ci>n</m:ci>
      <m:csymbol cd="setname1" name="Z"/>
    </m:apply>
    <m:apply>
      <m:csymbol cd="logic1" name="and"/>
      <m:apply>
        <m:csymbol cd="relation1" name="eq"/>
        <m:apply>
          <m:csymbol cd="arith1" name="power"/>
          <m:ci>x</m:ci>
          <m:cn>0</m:cn>
        </m:apply>
        <m:csymbol cd="alg1" name="one"/>
      </m:apply>
      <m:apply>
        <m:csymbol cd="relation1" name="eq"/>
        <m:apply>
          <m:csymbol cd="arith1" name="power"/>
          <m:ci>x</m:ci>
          <m:ci>n</m:ci>
        </m:apply>
        <m:apply>
          <m:csymbol cd="arith1" name="times"/>
          <m:ci>x</m:ci>
          <m:apply>
            <m:csymbol cd="arith1" name="power"/>
            <m:ci>x</m:ci>
            <m:apply>
              <m:csymbol cd="arith1" name="minus"/>
              <m:ci>n</m:ci>
              <m:cn>1</m:cn>
            </m:apply>
          </m:apply>
        </m:apply>
      </m:apply>
    </m:apply>
  </m:apply>
</m:math>
</mcd:formal>
    </mcd:property>
  </mcd:MMLDefinition>
  <mcd:MMLDefinition xml:id="" cdrole="application">
    <mcd:property>
      <mcd:description>
        <p> for all x,y | abs(x) + abs(y) &gt;= abs(x+y) </p>
      </mcd:description>
      <mcd:formal>
<m:math xmlns:m="http://www.w3.org/1998/Math/MathML" version="2.0" cdbase="http://www.openmath.org/cd">
  <m:bind><m:csymbol cd="quant1" name="forall"/><m:bvar>
      <m:ci>x</m:ci>
      <m:ci>y</m:ci>
    </m:bvar><m:apply>
      <m:csymbol cd="relation1" name="geq"/>
      <m:apply>
        <m:csymbol cd="arith1" name="plus"/>
        <m:apply>
          <m:csymbol cd="arith1" name="abs"/>
            <m:ci>x</m:ci>
        </m:apply>
        <m:apply>
          <m:csymbol cd="arith1" name="abs"/>
            <m:ci>y</m:ci>
        </m:apply>
      </m:apply>
      <m:apply>
        <m:csymbol cd="arith1" name="abs"/>
        <m:apply>
          <m:csymbol cd="arith1" name="plus"/>
          <m:ci>x</m:ci>
          <m:ci>y</m:ci>
        </m:apply>
      </m:apply>
    </m:apply></m:bind>
</m:math>
</mcd:formal>
    </mcd:property>
  </mcd:MMLDefinition>
  <mcd:MMLDefinition xml:id="" cdrole="application">
    <mcd:property>
      <mcd:description>
        <p> x\in C implies root(x,n) = exp(ln(x)/n) </p>
      </mcd:description>
      <mcd:formal>
  <m:math xmlns:m="http://www.w3.org/1998/Math/MathML" version="2.0" cdbase="http://www.openmath.org/cd">
    <m:apply>
      <m:csymbol cd="logic1" name="implies"/>
      <m:apply>
        <m:csymbol cd="set1" name="in"/>
        <m:ci>x</m:ci>
        <m:csymbol cd="setname1" name="C"/>
      </m:apply>
      <m:apply>
        <m:csymbol cd="relation1" name="eq"/>
        <m:apply>
          <m:csymbol cd="arith1" name="root"/>
          <m:ci>x</m:ci>
          <m:ci>n</m:ci>          
        </m:apply>
        <m:apply>
          <m:csymbol name="exp" cd="transc1"/>
          <m:apply>
            <m:csymbol name="divide" cd="arith1"/>
            <m:apply>
              <m:csymbol name="ln" cd="transc1"/>
              <m:ci>x</m:ci>
            </m:apply>
            <m:ci>n</m:ci>          
          </m:apply>
        </m:apply>
      </m:apply>
    </m:apply>
  </m:math>
</mcd:formal>
    </mcd:property>
    <mcd:property>
      <mcd:description>
        <p> for all a,n | power(root(a,n),n) = a (if the root exists!) </p>
      </mcd:description>
      <mcd:formal>
  <m:math xmlns:m="http://www.w3.org/1998/Math/MathML" version="2.0" cdbase="http://www.openmath.org/cd">
    <m:bind><m:csymbol cd="quant1" name="forall"/><m:bvar>
         <m:ci>a</m:ci>
         <m:ci>n</m:ci>
      </m:bvar><m:apply>
        <m:csymbol cd="relation1" name="eq"/>
        <m:apply>
          <m:csymbol cd="arith1" name="power"/>
          <m:apply>
            <m:csymbol cd="arith1" name="root"/>
            <m:ci>a</m:ci>
            <m:ci>n</m:ci>
          </m:apply>
          <m:ci>n</m:ci>
        </m:apply>
        <m:ci>a</m:ci>
      </m:apply></m:bind>
  </m:math>
</mcd:formal>
    </mcd:property>
