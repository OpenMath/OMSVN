  <notation cdbase="FIXME" cd="arith1" name="root">
    <prototype>
      <om:OMA>
        <om:OMS cd="arith1" name="root"/>
        <expr name="arg"/>
        <expr name="n"/>
      </om:OMA>
    </prototype>
    <prototype>
      <m:apply>
        <m:csymbol cd="arith1" name="root"/>
        <expr name="arg"/>
        <expr name="n"/>
      </m:apply>
    </prototype>
    <rendering>
      <m:mroot>
	<render name="arg"/>
	<render name="n"/> 
      </m:mroot>
    </rendering>
    <prototype>
      <om:OMS cd="arith1" name="root"/>
    </prototype>
    <prototype>
      <m:csymbol cd="arith1" name="root"/>
    </prototype>
    <rendering>
       <m:mo>√</m:mo>
    </rendering>
  </notation>
  <notation cdbase="FIXME" cd="arith1" name="sum">
    <prototype>
      <om:OMA>
	<om:OMS cd="arith1" name="sum" mcd:cr="fun"/>
	<om:OMA>
	  <om:OMS cd="interval1" name="integer_interval"/>
	  <expr name="low"/>
	  <expr name="high"/>
	</om:OMA>
	<om:OMBIND>
	  <om:OMS cd="fns1" name="lambda"/>
	  <om:OMBVAR><expr name="var"/></om:OMBVAR>
	  <expr name="scope"/>
      </om:OMBIND>
    </om:OMA>
    </prototype>
    <prototype>
      <m:apply>
	<m:csymbol cd="arith1" name="sum" mcd:cr="fun"/>
	<m:apply>
	  <m:csymbol cd="interval1" name="integer_interval"/>
	  <expr name="low"/>
	  <expr name="high"/>
	</m:apply>
	<m:bind>
	  <m:csymbol cd="fns1" name="lambda"/>
	  <m:bvar><expr name="var"/></m:bvar>
	  <expr name="scope"/>
	</m:bind>
      </m:apply>
    </prototype>
    <rendering context="displaystyle">
      <m:mrow>
	<m:munderover>
	  <m:mo mcd:cr="fun">∑</m:mo>
	  <m:mrow>
	    <render name="var"/>
	    <m:mo>=</m:mo>
	    <render name="low"/>
	  </m:mrow>
	  <render name="high"/>
	</m:munderover>
	<render name="scope"/>  
      </m:mrow>
    </rendering>
    <rendering context="textstyle">
      <m:mrow>
	<m:msubsup>
	  <m:mo mcd:cr="fun">∑</m:mo>
	  <m:mrow>
	    <render name="var"/>
	    <m:mo>=</m:mo>
	    <render name="low"/>
	  </m:mrow>
	  <render name="high"/>
	</m:msubsup>
	<render name="scope"/>  
      </m:mrow>
    </rendering>
    <prototype>
      <om:OMS cd="arith1" name="sum"/>
    </prototype>
    <prototype>
      <m:csymbol cd="arith1" name="sum"/>
    </prototype>
    <rendering>
      <m:mo>∑</m:mo>
    </rendering>
  </notation>

