      <om:OMS cd="arith1" name="power"/>
    </prototype>
    <prototype>
      <m:csymbol cd="arith1" name="power"/>
    </prototype>
    <rendering>
      <!--IMPROVEME: the following is just a fallback-->
      <m:mo>power</m:mo>
    </rendering>
    <prototype>
      <om:OMA>
        <om:OMS cd="arith1" name="power"/>
        <expr name="arg1"/>
