  <notation cdbase="FIXME" cd="arith1" name="abs">
    <prototype>
      <om:OMS cd="arith1" name="abs"/>
    </prototype>
    <prototype>
      <m:csymbol cd="arith1" name="abs"/>
    </prototype>
    <rendering>
      <!--IMPROVEME: the following is just a fallback-->
      <m:mo>abs</m:mo>
    </rendering>
    <prototype>
      <om:OMA>
        <om:OMS cd="arith1" name="abs" mcd:cr="fun"/>
